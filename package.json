{
  "name": "react-glue-redux",
  "version": "1.3.2",
  "description": "relate react to glue-redux,like react-redux",
  "main": "./cj/index.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --config webpack/webpack.config.dev.js",
    "build:es": "babel src --out-dir es",
    "build:cj": "cross-env BABEL_ENV=cj babel src --out-dir cj",
    "build": "webpack --config webpack/webpack.config.prod.js && npm run build:es && npm run build:cj"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "globals": {
      "BABEL_ENV": "test"
    }
  },
  "devDependencies": {
    "@babel/cli": "~7.2.0",
    "@babel/core": "~7.2.0",
    "@babel/plugin-proposal-class-properties": "~7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "~7.2.0",
    "@babel/plugin-transform-runtime": "~7.2.0",
    "@babel/preset-env": "~7.2.0",
    "@babel/preset-react": "~7.0.0",
    "@babel/runtime": "~7.2.0",
    "@types/react": "~16.4.18",
    "@types/redux": "~3.6.0",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "~8.2.5",
    "babel-jest": "~23.6.0",
    "babel-loader": "~7.1.4",
    "clean-webpack-plugin": "~0.1.19",
    "cross-env": "~5.2.0",
    "css-loader": "~0.28.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "~4.19.1",
    "eslint-config-airbnb": "~17.0.0",
    "eslint-loader": "~2.0.0",
    "eslint-plugin-import": "~2.13.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.10.0",
    "glue-redux": "^3.5.4",
    "html-webpack-plugin": "~3.2.0",
    "jest": "~23.6.0",
    "less": "~3.0.4",
    "less-loader": "~4.1.0",
    "normalize.css": "~8.0.0",
    "postcss-flexbugs-fixes": "~3.3.1",
    "postcss-loader": "~2.1.5",
    "prop-types": "~15.6.2",
    "react": "~16.6.3",
    "react-dom": "~16.6.3",
    "react-redux": "~5.1.0",
    "react-router-dom": "~4.3.1",
    "react-test-renderer": "~16.6.3",
    "redux": "~3.5.2",
    "redux-devtools": "~3.4.1",
    "redux-devtools-dock-monitor": "~1.1.3",
    "redux-devtools-log-monitor": "~1.4.0",
    "sinon": "^7.3.0",
    "source-map-loader": "~0.2.3",
    "style-loader": "~0.21.0",
    "uglifyjs-webpack-plugin": "~1.2.7",
    "webpack": "~4.15.0",
    "webpack-cli": "~2.0.4",
    "webpack-dev-server": "~3.1.4"
  },
  "peerDependencies": {
    "react": ">= 1.0.0",
    "glue-redux": ">= 3.5.4",
    "@types/react": ">= 15.x"
  },
  "files": [
    "umd",
    "es",
    "cj",
    "package.json",
    "README.md",
    "src",
    "index.d.ts"
  ],
  "keywords": [
    "redux",
    "glue-redux",
    "action",
    "reducer"
  ],
  "repository": "https://github.com/ZhouYK/react-glux.git",
  "author": "ink",
  "license": "MIT"
}
